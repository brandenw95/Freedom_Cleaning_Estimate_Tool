.confirm-button{
    width: 25%;
    background-color: #009ce6;
    border: 1px grey solid;
    border-radius: 40px;
    color: #fff;
    margin-top: 20px;
    margin-bottom: 20px;
}
.confirm-section{
    width: 100%;
    height:auto;
}
.confirm-section > img{
    position:absolute;
    top:0;
    left:0;
    z-index:0;
    width: 100%;
    height: 100%;
}
.confirm-columns{
    width: 100%;
    margin-top: 25vh;
}
.confirm-column-header{
    background-color: #fff;
    color: #0eb2e7;
    width: 50%;
    height: 75px;
    padding: 10px;
    position: relative;
    margin: auto;
    text-align: center;
    border-top-left-radius: 20px;
    border-top-right-radius: 20px;
    font-family: Quicksand;
    font-weight: 800;
    text-transform: uppercase;
    box-shadow: 0 -5px 5px -5px rgba(0,0,0,0.45),
    -5px 0 5px -5px rgba(0,0,0,0.45), 
    5px 0 5px -5px rgba(0,0,0,0.45);
}
.confirm-column-content{
    box-shadow: 0 5px 5px -5px rgba(0,0,0,0.45),
    -5px 0 5px -5px rgba(0,0,0,0.45), 
    5px 0 5px -5px rgba(0,0,0,0.45);
    font-family: Quicksand;
    font-size: 20px;
    padding: 40px;
    width: 50%;
    height: auto;
    text-align: center;
    background-color: rgba(255,255,255,0.6);
    margin:auto;
    border-bottom-left-radius: 20px;
    border-bottom-right-radius: 20px;
    display: block;
    position: relative;
}
.checkmark{
    position: relative;
    display: block;
    width: 100px;
    height: 100px;
    margin: 0 auto 40px auto;
}

.info > *{
    font-family: Quicksand;

}
.section{
    width: 100%;
    height: 2000px;
    background: url('bg8.jpg') no-repeat center center / cover;
    
    
}
table{
    table-layout: fixed ;
   
}
body{
    color: #000 !important;
}
.header > *{
    font-size: 20px;
    text-transform: uppercase;
   
}
td{
    text-align: left;
    text-transform: capitalize;
    font-weight: 700;
    width: 40% ;
    padding: 3px !important;
}
.container-list ul{
    margin:0;
}
.content-container{
    display: flex;
    flex-direction: column;
    align-items: center;
    text-align: center;
}
.desc{
    padding: 20px;
}
.desc ul li h4{
    font-weight: 400;
}
.info{
    padding: 20px;
    color: #fff;
}
.info h2{
    text-transform: uppercase;
}
.calculator-container{
    height: auto;
    width: 50vw;
    backdrop-filter: blur(5px);
    background-color: rgba(255, 255, 255, 0.212);
    border: white 0px solid;
    border-radius: 20px;
    box-shadow: 0px 0px 25px 1px rgba(0,0,0,0.45);
    

}
.calculator-content-header{
    display: flex;
    flex-wrap: wrap-reverse;
    align-items: center;
    justify-content: space-evenly;
    padding-bottom: 20px;
    padding-top: 20px;
    background-color: #fff;
    height: 20%;
    border-top-right-radius: 20px;
    border-top-left-radius: 20px;
}
.calculator-content-header img{
    max-width: 40%;
}
.header-text h3 .cost_value{
    text-transform: uppercase;
    color: #0eb2e7;
    font-family: Quicksand;
    font-size: 3.5vw;
    font-weight: 800;
    line-height: 4px;
}
.header-text p{
    font-family: Quicksand;
    
}
.header-text{
    width: 50%;
}
.calculator-content-menu-select{
    display: flex;
    width: 90%;
    margin:20px auto 20px auto;
    justify-content: space-between;
    align-items: center;
    text-transform: uppercase;
    color: #000;
    background-color: rgba(255, 255, 255, 0.2);
    border-radius: 20px;
    border: 1px #fff solid;
    flex-wrap: wrap;
    padding:10px;
}


a:hover, a:focus, a:active {
    text-decoration: none;
    color: #fff;
}
input[type="radio"] {
    display: none;
}
input[type="radio"]:checked + label {
    background-color: #fff;
    box-shadow: 0px 0px 25px 1px rgba(0,0,0,0.2);
    transition: background-color 0.5s ease, box-shadow 0.5s ease;
}
.calculator-content-menu-select > *{
    font-family: Quicksand;
    font-weight: bold;
    padding: 20px 5px 20px 5px;
    border-radius: 20px;
    transition: background-color 0.5s ease, box-shadow 0.5s ease;
    margin:0;
}
